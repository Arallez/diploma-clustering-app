        <rdfs:label xml:lang="ru">Алгоритм кластеризации</rdfs:label>
    </owl:Class>
    
    <!-- http://www.semanticweb.org/diploma/clustering#ClusterSize -->
    <owl:Class rdf:about="http://www.semanticweb.org/diploma/clustering#ClusterSize">
        <rdfs:label xml:lang="ru">Ожидаемый размер кластеров</rdfs:label>
    </owl:Class>
    
    <!-- http://www.semanticweb.org/diploma/clustering#DensityBasedAlgorithm -->
    <owl:Class rdf:about="http://www.semanticweb.org/diploma/clustering#DensityBasedAlgorithm">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/diploma/clustering#Algorithm"/>
        <rdfs:label xml:lang="ru">Плотностные алгоритмы</rdfs:label>
    </owl:Class>
    
    <!-- http://www.semanticweb.org/diploma/clustering#Geometry -->
    <owl:Class rdf:about="http://www.semanticweb.org/diploma/clustering#Geometry">
        <rdfs:label xml:lang="ru">Геометрия кластеров</rdfs:label>
    </owl:Class>
    
    <!-- http://www.semanticweb.org/diploma/clustering#HierarchicalAlgorithm -->
    <owl:Class rdf:about="http://www.semanticweb.org/diploma/clustering#HierarchicalAlgorithm">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/diploma/clustering#Algorithm"/>
        <rdfs:label xml:lang="ru">Иерархические алгоритмы</rdfs:label>
    </owl:Class>
    
    <!-- http://www.semanticweb.org/diploma/clustering#InferenceType -->
    <owl:Class rdf:about="http://www.semanticweb.org/diploma/clustering#InferenceType">
        <rdfs:label xml:lang="ru">Тип вывода (Inductive/Transductive)</rdfs:label>
    </owl:Class>
    
    <!-- http://www.semanticweb.org/diploma/clustering#Metric -->
    <owl:Class rdf:about="http://www.semanticweb.org/diploma/clustering#Metric">
        <rdfs:label xml:lang="ru">Метрика расстояния</rdfs:label>
    </owl:Class>
    
    <!-- http://www.semanticweb.org/diploma/clustering#ModelBasedAlgorithm -->
    <owl:Class rdf:about="http://www.semanticweb.org/diploma/clustering#ModelBasedAlgorithm">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/diploma/clustering#Algorithm"/>
        <rdfs:label xml:lang="ru">Модельные алгоритмы</rdfs:label>
    </owl:Class>
    
    <!-- http://www.semanticweb.org/diploma/clustering#Parameter -->
    <owl:Class rdf:about="http://www.semanticweb.org/diploma/clustering#Parameter">
        <rdfs:label xml:lang="ru">Параметры</rdfs:label>
    </owl:Class>
    
    <!-- http://www.semanticweb.org/diploma/clustering#PartitioningAlgorithm -->
    <owl:Class rdf:about="http://www.semanticweb.org/diploma/clustering#PartitioningAlgorithm">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/diploma/clustering#Algorithm"/>
        <rdfs:label xml:lang="ru">Алгоритмы разбиения</rdfs:label>
    </owl:Class>
    
    <!-- http://www.semanticweb.org/diploma/clustering#Scalability -->
    <owl:Class rdf:about="http://www.semanticweb.org/diploma/clustering#Scalability">
